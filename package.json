{
  "name": "browser-dev-integration",
  "version": "0.1.0",
  "description": "Ultra-low latency browser development integration",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:experiment": "vite --open /src/experiment/",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "cd scripts && ./test-system.sh",
    "setup": "cd scripts && ./setup.sh",
    "start:system": "cd scripts && ./start-system.sh",
    "stop:system": "cd scripts && ./stop-system.sh",
    "server:dev": "cd github-app && npm run dev",
    "server:start": "cd github-app && npm start",
    "clean": "rm -rf node_modules dist .vite",
    "test:browser": "echo 'Use BrowserTools MCP for browser testing - see docs/BROWSERTOOLS_GUIDE.md'",
    "test:debug": "echo 'Enter debugger mode with BrowserTools - see docs/BROWSERTOOLS_GUIDE.md'",
    "create:pattern-branch": "node scripts/create-pattern-branch.js",
    "commit:patterns": "node scripts/commit-patterns.js",
    "submit:patterns": "node scripts/submit-patterns.js",
    "clean:pending": "rm -rf src/experiment/components/pending/*",
    "export:approved": "node scripts/export-approved.js",
    "setup:contributor": "node scripts/setup-contributor.js"
  },
  "dependencies": {
    "@dbml/core": "^3.13.8-alpha.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@softwaretechnik/dbml-renderer": "^1.0.31",
    "mermaid": "^11.9.0",
    "protobufjs": "^7.2.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0",
    "simple-peer": "^9.11.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20.11.24",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/simple-peer": "^9.11.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.3.3",
    "vite": "^7.0.5"
  }
}
